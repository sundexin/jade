body, div, label, dl, dt, dd, ul, ol, li, h1, h2, h3, h4, h5, h6, pre, code, form, fieldset, legend, p, blockquote, th, td
  list-style none
  margin 0
  padding 0
   border none
   outline 0 none
body
  font-family:'Microsoft Yahei','宋体', sunsun,Arial, Helvetica, sans-serif
  font-size:12px
  line-height:22px
  color:#666666
  background:#fff
img
  border:none
.top
  padding 10px
  background #f60
  line-height 50px
  text-align center
  font-size 40px
.nav
  margin 20px 0
  border 1px solid #900
  overflow hidden
.nav ul li
      float left
      width 50px
      text-align center
.nav ul li a
          color red
.nav ul li a:hover
          color blue
#wrap
  width 960px
  border 1px solid #ccc
  overflow hidden
  margin 0 auto
  .left
      width 200px
      background #f2f2f2
      float left
      overflow hidden
  .right
      float left
      width 750px
      margin-left 10px
      background #f4f4f4
.t1
  font-size 30px
  padding-left 10px
.t2
  @extends .t1
  color red







